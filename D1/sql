create table record (
	record_id int(8) not null PRIMARY KEY AUTO_INCREMENT,
	date datetime not null,
  type varchar(25) not null,
  quantitiy int(3) not null,
  id int(8) not null,
  class varchar(3) not null,
  pss int(8) not null
);

insert into record(date, type, quantitiy, id, class, pss) values (now(),'AAA 000 BBB','999','1','3D2', '1');

  update record
  set return_date = now(), return_quantity = '999', return_pss = '1'
  where record_id = 1

UPDATE record SET return_date = now(), return_quantity = '1', return_pss = '1' WHERE record_id ='50';

